    "recruit-supplier": {
      "projectType": "application",
      "root": "apps/recruit-supplier",
      "sourceRoot": "apps/recruit-supplier/src",
      "prefix": "recruit-supplier",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/recruit-supplier",
            "index": "apps/recruit-supplier/src/index.html",
            "main": "apps/recruit-supplier/src/main.ts",
            "polyfills": "apps/recruit-supplier/src/polyfills.ts",
            "tsConfig": "apps/recruit-supplier/tsconfig.app.json",
            "assets": [
              "apps/recruit-supplier/src/favicon.ico",
              "apps/recruit-supplier/src/assets"
            ],
            "styles": [
              "node_modules/ng-zorro-antd/ng-zorro-antd.less",
              "apps/recruit-supplier/src/styles.scss",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "libs/ui-theme/src/lib/antd/themes/default.less",
              "libs/ui-theme/src/lib/antd/themes/dark.less",
              "./node_modules/quill/dist/quill.core.css",
              "./node_modules/quill/dist/quill.bubble.css",
              "./node_modules/quill/dist/quill.snow.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/ui-theme/src/lib"]
            },
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": false,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "dev": {
              "fileReplacements": [
                {
                  "replace": "libs/app-config/src/lib/environments/environment.ts",
                  "with": "libs/app-config/src/lib/environments/environment.dev.ts"
                }
              ]
            },
            "prd": {
              "fileReplacements": [
                {
                  "replace": "libs/app-config/src/lib/environments/environment.ts",
                  "with": "libs/app-config/src/lib/environments/environment.prd.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "100kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "recruit-supplier:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "recruit-supplier:build:dev"
            },
            "prd": {
              "browserTarget": "recruit-supplier:build:prd"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "recruit-supplier:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/recruit-supplier/src/**/*.ts",
              "apps/recruit-supplier/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/recruit-supplier"],
          "options": {
            "jestConfig": "apps/recruit-supplier/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
